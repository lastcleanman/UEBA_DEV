      "path": "/UEBA_DEV/data/logs/Auth_Logs*.log"
    },
    {
      "name": "Web_Logs",
      "type": "file",
      "path": "/UEBA_DEV/data/logs/Web_Logs*.log"
    },
    {
      "name": "Firewall_Logs",
      "type": "file",
      "path": "/UEBA_DEV/data/logs/Firewall_Logs*.log"
