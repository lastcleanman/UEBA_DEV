      "password": "Suju!0901",
      "database": "ueba_db",
      "table_name": "employees",
      "watermark_col": "emp_id",
      "watermark_default": "0"
    },
    {
      "name": "ueba_mariaDB",
      "type": "mariadb",
      "enabled": false,
      "host": "192.168.0.131",
      "port": "13306",
      "user": "ueba_user",
      "password": "Suju!0901",
      "database": "UEBA_TEST",
      "table_name": "sj_ueba_hr",
      "watermark_col": "emp_id",
      "watermark_default": "0"
    },
    {
      "name": "Auth_Logs",
